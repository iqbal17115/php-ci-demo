{
    "name": "iqbal/php-ci-demo",
    "description": "A simple PHP OOP project with PHPUnit tests and CI workflow",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3.17"
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
